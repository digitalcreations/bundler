{
    "name": "dc/bundler",
    "description": "Asset bundling for dc/router",
    "minimum-stability": "stable",
    "homepage": "http://github.com/digitalcreations/bundler",
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Creations AS",
            "homepage": "http://www.digitalcreations.no",
            "role": "Company"
        },
        {
            "name": "Vegard Andreas Larsen",
            "homepage": "http://vega.rd.no",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "http://github.com/digitalcreations/bundler/issues",
        "source": "http://github.com/digitalcreations/bundler"
    },
    "autoload": {
        "psr-4": {
            "DC\\Bundler\\": "src/Bundler/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DC\\Tests\\Bundler\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "require": {
        "dc/router": "*",
        "dflydev/apache-mime-types": "*"
    },
    "suggests": {
        "dc/ioc": "0.*"
    }
}